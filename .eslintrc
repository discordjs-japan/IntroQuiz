{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "accessor-pairs": 2,
        "array-bracket-spacing": 2,
        "arrow-parens": 2,
        "arrow-spacing": 2,
        "block-scoped-var": 2,
        "block-spacing": 2,
        "brace-style": 2,
        "camelcase": 2,
        "comma-dangle": 2,
        "comma-spacing": 2,
        "comma-style": 2,
        "computed-property-spacing": 2,
        "consistent-this": 2,
        "constructor-super": 2,
        "dot-location": 2,
        "eol-last": 2,
        "eqeqeq": 2,
        "func-names": 2,
        "func-style": [
            2,
            "declaration",
            {
                "allowArrowFunctions": true
            }
        ],
        "generator-star-spacing": 2,
        "id-length": 2,
        "id-match": [
            2,
            "^[a-z]+([A-Z][a-z]+)*$",
            {
                "properties": false
            }
        ],
        "indent": 0,
        "key-spacing": 2,
        "lines-around-comment": 0,
        "no-alert": 2,
        "no-class-assign": 2,
        "no-console": 0,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-lonely-if": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": 2,
        "no-nested-ternary": 0,
        "no-new-object": 2,
        "no-restricted-syntax": 2,
        "no-spaced-func": 2,
        "no-ternary": 0,
        "no-this-before-super": 2,
        "no-trailing-spaces": 2,
        "no-underscore-dangle": 2,
        "no-unneeded-ternary": 2,
        "no-unreachable": 2,
        "no-var": 2,
        "object-curly-spacing": 2,
        "object-shorthand": 2,
        "one-var": 2,
        "operator-assignment": 2,
        "operator-linebreak": 2,
        "padded-blocks": 0,
        "prefer-arrow-callback": 2,
        "prefer-const": 2,
        "prefer-reflect": 2,
        "prefer-spread": 2,
        "quote-props": 2,
        "quotes": [
            2,
            "backtick"
        ],
        "require-jsdoc": 0,
        "require-yield": 2,
        "semi": 2,
        "semi-spacing": 2,
        "sort-vars": 0,
        "space-after-keywords": 0,
        "space-before-blocks": 2,
        "space-before-keywords": 0,
        "space-in-parens": 2,
        "space-infix-ops": 2,
        "space-return-throw-case": 0,
        "space-unary-ops": 0,
        "spaced-comment": 0,
        "strict": 2,
        "wrap-regex": 2,
        "no-undef": 2
    }
}
